# syntax
syntax enable

# clipboard
set clipboard=unnamed

# indent
filetype plugin indent on
set autoindent smartindent

# tab
set expandtab
set tabstop=2 softtabstop=2 shiftwidth=2

# ruler
set number ruler


  1 set tabstop=2 softtabstop=2 shiftwidth=2
  2 set expandtab
  3 set number ruler
  4 set autoindent smartindent
  5 syntax enable
  6 filetype plugin indent on
  7 set clipboard=unnamed


syntax enable
set clipboard=unnamed
filetype plugin indent on
set autoindent smartindent
set expandtab
set tabstop=2 softtabstop=2 shiftwidth=2
set number ruler

